[Variables]

;=== USERNAME ===
Username=
;Enter your Reddit username

;=== URL TO REDDIT RSS FEED ===
URL=
;Any Reddit page (with posts + non-private) URL + .rss
;Refer to readme for further details

;=== TWITCH OAUTH TOKEN ===
OAuth=
;OAuth Twitch token, can be generated via Authorization skin

;=== URL TO IMAGE-BASED RSS ===
URLSingle=
;Place an image-only rss feed url here
;Single RSS is in developement hell

;=== FONT CHOICE ===
Font=8
;1 - Microsoft JhengHei UI
;2 - Century Gothic
;3 - Eras Medium ITC
;4 - Futura Lt
;5 - Lato
;6 - Montserrat
;7 - Montserrat Light
;8 - Quicksand Medium
;9 - Segoe UI
;10 - Comfortaa Light

;=== FEED SETTINGS ===
EntrySpacing=5
LineSpacing=5
;Spacing between objects, values are specified in pixels
